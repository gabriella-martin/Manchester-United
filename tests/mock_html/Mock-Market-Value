                %image%
                <div class="player-desc">
                    <div class="ttl">
                        <strong>%title%</strong>
                        <span>%title_sub%</span>
                    </div>
                </div>
                %value%
            </a>`;

        const variables = {
            '%slug%': URL,
            '%title%': result.title,
            '%title_sub%': result.title_sub,
            '%image%': image,
            '%value%': playerValue
