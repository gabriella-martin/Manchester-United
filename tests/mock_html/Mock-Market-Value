    function createTeamCompactSearchResult(result) {
        const URL = 'https://www.footballtransfers.com/en/teams/' + result.slug;

        const img = !!result.image ? `<i class="plyr-prfl-img"><img src="${result.image}" alt="${result.title}" /></i>` : '';
